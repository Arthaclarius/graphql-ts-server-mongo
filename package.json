{
  "name": "graphql-ts-server-mongo",
  "version": "0.0.1",
  "main": "dist/server.js",
  "repository": "https://github.com/Arthaclarius/graphql-ts-server-mongo.git",
  "author": "Arthaclarius <sgcg5@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "SET NODE_ENV=develop&& nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start:test": "start-server-and-test test:server http://localhost:4040 test",
    "build:graphql": "gql2ts src/graphql/modules/*/*.graphql -o src/graphql/types/schema.d.ts",
    "test": "SET NODE_ENV=test&& jest --watch",
    "test:server": "SET NODE_ENV=test&& ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.3.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "graphql-import": "^0.6.0",
    "graphql-request": "^1.6.0",
    "graphql-tools": "^3.0.2",
    "graphql-yoga": "^1.14.5",
    "ioredis": "^3.2.2",
    "mongoose": "^5.1.2",
    "mongoose-simple-decorator": "2.0.1",
    "multer": "^1.3.0",
    "node-fetch": "^2.1.2",
    "rate-limit-redis": "^1.5.0",
    "reflect-metadata": "^0.1.12",
    "request-promise": "^4.2.2",
    "routing-controllers": "^0.7.7",
    "sparkpost": "^2.1.2",
    "tslint": "^5.10.0",
    "type-graphql": "^0.12.1",
    "typedi": "^0.8.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/connect-redis": "^0.0.7",
    "@types/dotenv": "^4.0.3",
    "@types/express": "4.11.1",
    "@types/express-rate-limit": "^2.9.3",
    "@types/express-session": "^1.15.10",
    "@types/faker": "^4.1.2",
    "@types/ioredis": "^3.2.8",
    "@types/jest": "^23.0.0",
    "@types/mongoose": "^5.0.14",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.1.2",
    "@types/node-fetch": "^2.1.1",
    "@types/request-promise": "^4.1.41",
    "@types/sparkpost": "^2.1.4",
    "@types/uuid": "^3.4.3",
    "gql2ts": "^1.8.0",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "start-server-and-test": "^1.4.1",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.5",
    "tsconfig-paths": "^3.4.2",
    "typescript": "2.9.1",
    "yarn": "^1.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
